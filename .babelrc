{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "sourceMaps": true,
  "retainLines": true,
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  },
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}