{
  "name": "express-mongodb-react-redux-universal-starter",
  "version": "0.1.0",
  "description": "Starter for react, redux, universal rendering, express and mongodb",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/snaerth/express-mongodb-react-redux-universal-starter.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "react",
    "redux",
    "universal rendering",
    "mongodb"
  ],
  "author": "Snær Seljan Þóroddsson",
  "license": "MIT",
  "bugs": {
    "url": "/issues"
  },
  "homepage": "",
  "scripts": {
    "start-prod": "cross-env NODE_ENV=production npm-run-all --parallel start-prod-server start-prod-api",
    "start-prod-server": "cross-env HOST=localhost PORT=3000 node build/server.js",
    "start-prod-api": "cross-env HOST=localhost PORT=3030 node build/api.js",
    "start-dev": "cross-env NODE_ENV=development npm-run-all --parallel start-dev-server start-dev-api lint:watch",
    "start-dev-server": "babel-node src/server/index.js",
    "start-dev-api": "babel-node src/api/index.js",
    "build": "rimraf build && npm run build-client && npm run build-server && npm run build-api",
    "build-client": "webpack -p --config ./tools/webpack.client.prod.js --progress --profile --colors",
    "build-server": "webpack -p --config  ./tools/webpack.server.prod.js --progress --profile --colors",
    "build-api": "webpack -p --config  ./tools/webpack.api.prod.js --progress --profile --colors",
    "lint": "node_modules/.bin/esw webpack.client.dev.* src --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-preset-react-optimize": "^1.0.1",
    "bcrypt-nodejs": "0.0.3",
    "core-decorators": "^0.15.0",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-rate-limit": "^2.6.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "helmet": "^3.4.0",
    "http-proxy": "^1.16.2",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.22",
    "mongoose": "^4.7.8",
    "morgan": "^1.8.1",
    "node-fetch": "^1.6.3",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "serialize-javascript": "^1.3.0",
    "source-map-support": "^0.4.11"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.0",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.16.1",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "compression": "^1.6.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "helmet": "^3.4.0",
    "hpp": "^0.2.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.4.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-node-externals": "^1.5.4"
  }
}
