{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/src/admin/server.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": [
        "--harmony",
        "--no-deprecation"
      ],
      "sourceMaps": true,
      "protocol": "inspector",
      "smartStep": true,
      "outFiles": [
        "${workspaceRoot}/dist/**/*.js"
      ]
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": false,
      "sourceMaps": false,
      "outFiles": [],
      "protocol": "inspector",
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "port": 5858,
      "protocol": "inspector",
      "sourceMaps": false,
      "outFiles": []
    }
  ]
}